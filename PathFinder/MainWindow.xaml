<Window x:Class="PathFinder.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PathFinder"
        mc:Ignorable="d"
        Title="MainWindow" Height="1333" Width="1289">
    <StackPanel Margin="0,0,0,223">

        <Canvas x:Name="canvas" Width="1250" Height="1020" Margin="0 10" Background="Beige">
        </Canvas>
        <StackPanel Orientation="Horizontal" Margin="10 0 0 10" Width="650">
            <Button Content="Generate Map" Height="30" Width="100" Margin="0 0 0 10" Click="GenerateMap_Click" />
            <Button Content="Navigate Path" Height="30" Width="100" Margin="20 -10 0 0" Click="NavigateMap_Click" />
        </StackPanel>
    </StackPanel>
</Window>
